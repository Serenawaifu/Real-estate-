{
  "name": "DGrealtors",
  "version": "1.0.0",
  "private": true,
  "description": "DGrealtors has been in the real estate business for over 14 years, serving some of the industry's most prominent businesses.",
  "author": "DGrealtors <contact@dgrealtors.com>",
  "license": "MIT",
  "keywords": [
    "dgrealtors",
    "real-estate",
    "commercial-property",
    "property-leasing",
    "react",
    "vite",
    "tailwindcss"
  ],
  "homepage": "https://dgrealtors.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgrealtors/dgrealtors-website.git"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite preview --port 5000",
    "start": "npm run dev",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules package-lock.json",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:css": "stylelint \"**/*.{css,scss}\" --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "analyze": "vite-bundle-visualizer",
    "optimize:images": "node scripts/optimizeImages.js",
    "generate:sitemap": "node scripts/generateSitemap.js",
    "generate:robots": "node scripts/generateRobots.js",
    "update:placeholders": "node scripts/updatePlaceholders.js",
    "build:analyze": "npm run build && npm run analyze",
    "build:production": "npm run clean && npm run optimize:images && npm run build && npm run generate:sitemap",
    "deploy:preview": "npm run build && netlify deploy",
    "deploy:production": "npm run build:production && netlify deploy --prod",
    "check:deps": "npm-check-updates",
    "update:deps": "npm-check-updates -u && npm install",
    "security:check": "npm audit",
    "security:fix": "npm audit fix",
    "lighthouse": "lighthouse http://localhost:5173 --view",
    "prebuild": "npm run lint && npm run format:check",
    "postinstall": "husky install",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "framer-motion": "^11.2.10",
    "react-spring": "^9.7.3",
    "@react-spring/parallax": "^9.7.3",
    "swiper": "^11.1.4",
    "react-hook-form": "^7.52.0",
    "yup": "^1.4.0",
    "@hookform/resolvers": "^3.6.0",
    "react-intersection-observer": "^9.10.3",
    "react-lazy-load-image-component": "^1.6.2",
    "aos": "^2.3.4",
    "tsparticles": "^3.4.0",
    "@tsparticles/react": "^3.0.0",
    "@emailjs/browser": "^4.3.3",
    "axios": "^1.7.2",
    "react-helmet-async": "^2.0.5",
    "clsx": "^2.1.1",
    "react-hot-toast": "^2.4.1",
    "zustand": "^4.5.2",
    "date-fns": "^3.6.0",
    "react-countup": "^6.5.3",
    "disable-devtool": "^0.3.8",
    "lottie-react": "^2.4.0",
    "react-use": "^17.5.0",
    "react-parallax": "^3.5.1",
    "react-tilt": "^1.0.2",
    "@formspree/react": "^2.5.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/node": "^20.14.2",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "vite": "^5.3.1",
    "vite-plugin-pwa": "^0.20.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-tsconfig-paths": "^4.3.2",
    "vite-bundle-visualizer": "^1.2.1",
    "typescript": "^5.4.5",
    "tailwindcss": "^3.4.4",
    "postcss": "^8.4.38",
    "autoprefixer": "^10.4.19",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^9.5.14",
    "cssnano": "^7.0.2",
    "eslint": "^9.5.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-config-prettier": "^9.1.0",
    "@typescript-eslint/parser": "^7.13.1",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "stylelint": "^16.6.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-tailwindcss": "^0.0.1",
    "sharp": "^0.33.4",
    "imagemin": "^9.0.0",
    "imagemin-webp": "^8.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-svgo": "^11.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "npm-check-updates": "^16.14.20",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "serve": "^14.2.3",
    "netlify-cli": "^17.26.2"
  },
  "optionalDependencies": {
    "@tanstack/react-query": "^5.45.1",
    "vitest": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/user-event": "^14.5.2",
    "@playwright/test": "^1.44.1",
    "lighthouse": "^12.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
  }
