{
  "name": "DGrealtors",
  "version": "1.0.0",
  "private": true,
  "description": "DGrealtors has been in the real estate business for over 14 years, serving some of the industry's most prominent businesses.",
  "author": "DGrealtors <contact@dgrealtors.com>",
  "license": "MIT",
  "keywords": [
    "dgrealtors",
    "real-estate",
    "commercial-property",
    "property-leasing",
    "react",
    "vite",
    "tailwindcss"
  ],
  "homepage": "https://dgrealtors.in",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Serenawaifu/Real-estate-"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite preview --port 5000",
    "start": "npm run dev",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules package-lock.json",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:css": "stylelint \"**/*.{css,scss}\" --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "analyze": "vite-bundle-visualizer",
    "optimize:images": "node scripts/optimizeImages.js",
    "generate:sitemap": "node scripts/generateSitemap.js",
    "generate:robots": "node scripts/generateRobots.js",
    "update:placeholders": "node scripts/updatePlaceholders.js",
    "build:analyze": "npm run build && npm run analyze",
    "build:production": "npm run clean && npm run optimize:images && npm run build && npm run generate:sitemap",
    "deploy:preview": "npm run build && netlify deploy",
    "deploy:production": "npm run build:production && netlify deploy --prod",
    "check:deps": "npm-check-updates",
    "update:deps": "npm-check-updates -u && npm install",
    "security:check": "npm audit",
    "security:fix": "npm audit fix",
    "lighthouse": "lighthouse http://localhost:5173 --view",
    "prebuild": "npm run lint && npm run format:check",
    "postinstall": "husky install",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.0",
    "framer-motion": "^11.0.3",
    "react-spring": "^9.7.3",
    "@react-spring/parallax": "^9.7.3",
    "swiper": "^11.0.5",
    "react-hook-form": "^7.49.3",
    "yup": "^1.3.3",
    "@hookform/resolvers": "^3.3.4",
    "react-intersection-observer": "^9.5.3",
    "react-lazy-load-image-component": "^1.6.0",
    "aos": "^2.3.4",
    "tsparticles": "^3.1.0",
    "@tsparticles/react": "^3.0.0",
    "@emailjs/browser": "^3.12.1",
    "axios": "^1.6.5",
    "react-helmet-async": "^2.0.4",
    "clsx": "^2.1.0",
    "react-hot-toast": "^2.4.1",
    "zustand": "^4.5.0",
    "date-fns": "^3.3.0",
    "react-countup": "^6.5.0",
    "disable-devtool": "^0.3.8",
    "lottie-react": "^2.4.0",
    "react-use": "^17.4.3",
    "react-parallax": "^3.5.1",
    "react-tilt": "^1.0.2",
    "@formspree/react": "^2.5.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/node": "^20.11.5",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "vite": "^5.0.12",
    "vite-plugin-pwa": "^0.17.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-tsconfig-paths": "^4.3.1",
    "vite-bundle-visualizer": "^1.0.1",
    "typescript": "^5.3.3",
    "tailwindcss": "^3.4.1",
    "postcss": "^8.4.33",
    "autoprefixer": "^10.4.17",
    "postcss-import": "^16.0.0",
    "postcss-preset-env": "^9.3.0",
    "cssnano": "^6.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-config-prettier": "^9.1.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "stylelint": "^16.2.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-tailwindcss": "^0.0.1",
    "sharp": "^0.33.2",
    "imagemin": "^9.0.0",
    "imagemin-webp": "^8.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "npm-check-updates": "^16.14.12",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "serve": "^14.2.1",
    "netlify-cli": "^17.15.0"
  },
  "optionalDependencies": {
    "@tanstack/react-query": "^5.18.0",
    "vitest": "^1.2.1",
    "@vitest/ui": "^1.2.1",
    "@vitest/coverage-v8": "^1.2.1",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@playwright/test": "^1.41.0",
    "lighthouse": "^11.5.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "plugins": ["prettier-plugin-tailwindcss"]
  }
}
